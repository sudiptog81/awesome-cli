{
  "name": "@sudiptog81/awesome-cli",
  "version": "0.1.2",
  "description": "A CLI written in TypeScript to fetch GitHub repositories.",
  "private": false,
  "main": "dist/cli.js",
  "author": {
    "name": "Sudipto Ghosh",
    "email": "sudipto@ghosh.pro",
    "url": "https://github.com/sudiptog81"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudiptog81/awesome-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sudiptog81/awesome-cli/issues"
  },
  "homepage": "https://github.com/sudiptog81/awesome-cli",
  "keywords": [
   "typescript",
   "github"
  ],
  "bin": {
    "awesome-cli": "dist/cli.js"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./dist/cli.js",
    "build": "tsc && chmod +x dist/cli.js",
    "lint": "eslint --fix src/**/*.{js,ts}",
    "format": "prettier --write '**'",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@octokit/graphql": "^4.6.4",
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "ts-dedent": "^2.1.1",
    "yargs": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**": [
      "prettier --write",
      "git add"
    ]
  }
}
